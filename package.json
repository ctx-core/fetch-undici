{
	"name": "@ctx-core/fetch-undici",
	"version": "3.1.296",
	"description": "ctx-core fetch using undici",
	"keywords": [
		"ctx-core",
		"fetch",
		"undici"
	],
	"homepage": "https://github.com/ctx-core/fetch-undici#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/fetch-undici/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/fetch-undici.git"
	},
	"license": "Apache-2.0",
	"author": {
		"name": "Brian Takita",
		"url": "https://briantakita.me",
		"email": "info+ctx-core-fetch-undici@briantakita.me"
	},
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./fetch_response_pair",
		"./headers",
		"./RequestInit",
		"./undici-fetch"
	],
	"types": "./index.d.ts",
	"exports": {
		".": {
			"browser": "./index.browser.js",
			"default": "./index.js",
			"node": "./index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm run /^test:/",
		"test:types": "check-dts",
		"test:unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"disable:test:unit:coverage": "c8 pnpm test:unit"
	},
	"dependencies": {
		"@ctx-core/package": "^9.3.225",
		"ctx-core": "^7.2.31",
		"undici": "^6.19.8"
	},
	"devDependencies": {
		"c8": "^10.1.2",
		"check-dts": "^0.8.2",
		"tsx": "^4.19.0",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public"
	},
	"sideEffects": false
}