		"@ctx-core/sleep": "^8.2.44",
		"undici": "^6.0.1"
	},
	"devDependencies": {
		"c8": "^8.0.1",
		"check-dts": "^0.7.2",
		"tsx": "^4.6.2",
		"typescript": "next",
		"uvu": "^0.5.6"
