{
	"name": "@ctx-core/fetch-undici",
	"version": "3.1.84",
	"description": "ctx-core fetch using undici",
	"keywords": [
		"ctx-core",
		"fetch",
		"undici"
	],
	"homepage": "https://github.com/ctx-core/fetch-undici#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/fetch-undici/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/fetch-undici.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./fetch_response_pair",
		"./headers",
		"./RequestInit",
		"./undici-fetch"
	],
	"types": "./index.d.ts",
	"exports": {
		".": {
			"browser": "./index.browser.js",
			"default": "./index.js",
			"node": "./index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
